<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                            http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="test-seed-countries" author="test-user">
        <insert tableName="countries">
            <column name="name" value="Test Country"/>
            <column name="code" value="TC"/>
        </insert>
        <insert tableName="countries">
            <column name="name" value="Another Land"/>
            <column name="code" value="AL"/>
        </insert>
    </changeSet>

    <changeSet id="test-seed-items" author="test-user">
        <insert tableName="items">
            <column name="name" value="Test Phone"/>
            <column name="price" value="50000"/>
            <column name="quantity" value="10"/>
            <column name="manufacturer_id" valueNumeric="1"/>
        </insert>

        <insert tableName="items">
            <column name="name" value="Test Laptop"/>
            <column name="price" value="150000"/>
            <column name="quantity" value="5"/>
            <column name="manufacturer_id" valueNumeric="2"/>
        </insert>
    </changeSet>

</databaseChangeLog>